#include <iostream>
#include <fstream>
#include <string>
#include <vector>
#include <string>
#include <sstream>

using namespace std;

class Edge {

	private:
		int name;

	public:
		Edge(int name) {
			this->name = name;
		}

		int getName(){
			return this->name;
		}
};

class Vertex {
	private:
		int from, to;

	public:
		Vertex(int from, int to) {
			this->from = from;
			this->to = to;
		}

		int getFrom(){
			return this->from;
		}

		int getTo(){
			return this->to;
		}
};

vector<string> getLine(string line) {
	string str("Split me by whitespaces");
	string buf; // Have a buffer string
	stringstream ss(str); // Insert the string into a stream

	vector<string> tokens; // Create vector to hold our words

	while (ss >> buf)
		tokens.push_back(buf);

	return tokens;
}

int main() {
	string line;
	ifstream myfile("testfile");
	if (myfile.is_open()) {

		while (getline(myfile, line)) {
			cout << line << '\n';
		}
		myfile.close();

		Edge e1(1);
		Edge e2(2);
		Vertex v(1, 2);
	}

	else
		cout << "Unable to open file";

	return 0;
}

