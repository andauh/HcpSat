#include <iostream>
#include <fstream>
#include <string>

using namespace std;

int main () {
  string line;
  ifstream myfile ("testfile");
  if (myfile.is_open())
  {
    while ( getline (myfile,line) )
    {
      cout << line << '\n';
    }
    myfile.close();
  }

  else cout << "Unable to open file"; 

  return 0;
}

class Edge{
	private:
		int name;

	public:
		Edge(int arg){
			this->name = arg;
		}
};

class Vertex {
	private:
		int from, to;

	public:
		Vertex(int from, to){
			this->from = from;
			this->to = to;
		}
};
