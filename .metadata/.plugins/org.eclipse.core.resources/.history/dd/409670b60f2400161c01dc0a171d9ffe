#include <iostream>
#include <fstream>
#include <string>

using namespace std;

typefed struct 

int main () {
  string line;
  ifstream myfile ("testfile");
  if (myfile.is_open())
  {
    while ( getline (myfile,line) )
    {
      cout << line << '\n';
    }
    myfile.close();
  }

  else cout << "Unable to open file"; 

  return 0;
}